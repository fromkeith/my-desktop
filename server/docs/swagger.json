{
    "swagger": "2.0",
    "info": {
        "description": "API Calls",
        "title": "Desktop Eamil",
        "termsOfService": "http://fromkeith.com",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:5173",
    "basePath": "/api",
    "paths": {
        "/gmail/inbox": {
            "get": {
                "description": "List the user's email inbox",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "email"
                ],
                "summary": "List the inbox",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/GmailEntry"
                            }
                        }
                    }
                }
            }
        },
        "/gmail/message/:messageId": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "email"
                ],
                "summary": "Get the basic information about a message",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/GmailEntry"
                        }
                    }
                }
            }
        },
        "/gmail/message/:messageId/contents": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "email"
                ],
                "summary": "Get the contents of a message",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/GmailEntryBody"
                        }
                    }
                }
            }
        },
        "/gmail/thread/{threadId}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "email"
                ],
                "summary": "List all messages in a thread",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/GmailEntry"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "GmailEntry": {
            "type": "object",
            "required": [
                "additionalReceivers",
                "createdAt",
                "headers",
                "historyId",
                "internalDate",
                "labels",
                "messageId",
                "receivedAt",
                "receiver",
                "revisionCount",
                "sender",
                "snippet",
                "subject",
                "threadId",
                "updatedAt",
                "userId"
            ],
            "properties": {
                "additionalReceivers": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/PersonInfo"
                        }
                    }
                },
                "createdAt": {
                    "type": "string"
                },
                "headers": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "historyId": {
                    "type": "integer"
                },
                "internalDate": {
                    "type": "integer"
                },
                "labels": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "messageId": {
                    "type": "string"
                },
                "receivedAt": {
                    "type": "string"
                },
                "receiver": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/PersonInfo"
                    }
                },
                "replyTo": {
                    "$ref": "#/definitions/PersonInfo"
                },
                "revisionCount": {
                    "type": "integer"
                },
                "sender": {
                    "$ref": "#/definitions/PersonInfo"
                },
                "snippet": {
                    "type": "string"
                },
                "subject": {
                    "type": "string"
                },
                "threadId": {
                    "type": "string"
                },
                "updatedAt": {
                    "description": "For Sync + Conflict Resolution",
                    "type": "string"
                },
                "userId": {
                    "type": "string"
                }
            }
        },
        "GmailEntryBody": {
            "type": "object",
            "required": [
                "createdAt",
                "hasAttachments",
                "messageId",
                "revisionCount",
                "updatedAt",
                "userId"
            ],
            "properties": {
                "attachmentIds": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "createdAt": {
                    "type": "string"
                },
                "hasAttachments": {
                    "type": "integer"
                },
                "html": {
                    "type": "string"
                },
                "messageId": {
                    "type": "string"
                },
                "plainText": {
                    "type": "string"
                },
                "revisionCount": {
                    "type": "integer"
                },
                "updatedAt": {
                    "description": "For Sync + Conflict Resolution",
                    "type": "string"
                },
                "userId": {
                    "type": "string"
                }
            }
        },
        "PersonInfo": {
            "type": "object",
            "required": [
                "email",
                "name"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            }
        }
    }
}